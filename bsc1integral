  const PASSWORD = '123'; // required password
  const lockedScreen = document.getElementById('locked-screen');
  const content = document.getElementById('content');
  const msg = document.getElementById('msg');
  document.getElementById('unlock').addEventListener('click', tryUnlock);
  document.getElementById('pw').addEventListener('keydown', e=>{ if(e.key==='Enter') tryUnlock(); });

  function tryUnlock(){
    const val = document.getElementById('pw').value;
    if(val === PASSWORD){ lockedScreen.classList.add('hidden'); content.classList.remove('hidden'); }
    else { msg.textContent = 'Incorrect password — गलत पासवर्ड'; msg.classList.add('locked'); }
  }

